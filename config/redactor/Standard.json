{
  "buttons": ["format", "bold", "italic", "lists", "link", "file"],
  "formatting": ["p", "h3", "h4", "h5", "h6", "pre", "blockquote"],
  "plugins": ["customstyles", "fontcolor", "video", "widget", "table"],
  "fontcolors": [
    "#292929",
    "#F1F1F1",
    "#4c59a4",
    "#577590",
    "#43AA8B",
    "#90BE6D",
    "#F9C74F",
    "#F8961E",
    "#F3722C",
    "#F94144",
    "#d17b88"
  ],
  "customStyles": {
    "style": {
      "dropdown": {
        "marked": {
          "args": "mark"
        },
        "code": {
          "args": "code"
        },
        "variable": {
          "args": "var"
        },
        "shortcut": {
          "args": "kbd"
        },
        "deleted": {
          "args": "del"
        },
        "inserted": {
          "args": "ins"
        },
        "superscript": {
          "args": "sup"
        },
        "subscript": {
          "args": "sub"
        },
        "quote": {
          "args": "q"
        },
        "sourceCitation": {
          "args": "cite"
        },
        "smallPrint": {
          "args": "small"
        },
        "clearStyles": {
          "api": "module.inline.clearformat"
        }
      }
    },
    "quote": {
      "args": "q"
    },
    "clearStyles": {
      "api": "module.inline.clearformat"
    },
    "align": {
      "dropdown": {
        "alignLeft": {
          "api": "module.block.format",
          "args": "p"
        },
        "alignRight": {
          "api": "module.block.format",
          "args": {
            "tag": "p",
            "class": "align-right"
          }
        },
        "alignCenter": {
          "api": "module.block.format",
          "args": {
            "tag": "p",
            "class": "align-center"
          }
        },
        "alignJustify": {
          "api": "module.block.format",
          "args": {
            "tag": "p",
            "class": "align-justify"
          }
        }
      }
    }
  },
  "pastePlainText": true,
  "toolbarFixed": false,
  "source": true,
  "linkNewTab": true
}