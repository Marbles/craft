{
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.4.1",
    "@fortawesome/fontawesome-pro": "^5.4.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.6",
    "@fortawesome/vue-fontawesome": "^0.1.1",
    "axios": "^0.18.0",
    "jquery": "^3.2.1",
    "lazysizes": "^4.1.4",
    "npm": "^6.4.1",
    "picturefill": "^3.0.2",
    "tailwindcss": "^0.6.5",
    "vue": "^2.5.9"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.1.6",
    "critical": "^1.3.4",
    "cross-env": "^5.1.3",
    "eslint": "^5.7.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-vue": "^4.7.1",
    "git-rev-sync": "^1.11.1",
    "html-critical-webpack-plugin": "^2.1.0",
    "imagemin": "^6.0.0",
    "laravel-mix": "^4.0.0-beta.2",
    "laravel-mix-banner": "^0.1.0",
    "laravel-mix-critical": "^0.1.0",
    "laravel-mix-eslint": "^0.1.1",
    "laravel-mix-purgecss": "^3.0.0",
    "laravel-mix-tailwind": "^0.1.0",
    "moment": "^2.22.1",
    "postcss-object-fit-images": "^1.1.2",
    "purgecss-webpack-plugin": "^1.3.1",
    "tiny-cookie": "^2.1.1"
  },
  "eslintConfig": {
    "extends": [
      "plugin:prettier/recommended",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 2017
    },
    "env": {
      "es6": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "scripts": {
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "cross-env NODE_ENV=development webpack --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "hot": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production webpack --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  }
}
